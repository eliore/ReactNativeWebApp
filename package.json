{
  "name": "ReactNativeWebApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "cd web && webpack-dev-server",
    "start": "react-native start",
    "test": "jest",
    "lint": "tslint -p \".\" -t stylish",
    "build:web": "cd web && webpack"
  },
  "dependencies": {
    "lodash": "4.17.15",
    "mobx": "5.15.4",
    "mobx-react": "6.2.2",
    "moment": "2.29.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "0.62.2",
    "react-native-web": "0.12.3",
    "react-router": "5.2.0",
    "react-router-native": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-spring": "8.0.27",
    "react-data-table-component": "6.9.3",
    "@ckeditor/ckeditor5-react": "2.1.0",
    "@ckeditor/ckeditor5-build-classic": "19.0.2",
    "styled-components": "5.1.1",
    "prop-types": "15.7.2"
  },
  "devDependencies": {
    "babel-loader": "8.1.0",
    "@babel/core": "7.10.2",
    "@babel/runtime": "7.10.2",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-react": "7.10.1",
    "@babel/preset-typescript": "7.10.1",
    "@babel/plugin-transform-runtime": "7.10.1",
    "@babel/plugin-proposal-decorators": "7.10.1",
    "@babel/plugin-proposal-class-properties":"7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/react": "16.9.36",
    "@types/react-native": "0.62.13",
    "@types/node": "14.0.13",
    "@types/react-dom": "16.9.8",
    "@types/lodash": "4.14.155",
    "@types/react-router": "5.1.7",
    "@types/react-router-native": "5.1.0",
    "@types/react-router-dom": "5.1.5",
    "@types/styled-components": "5.1.0",
    "@types/jest": "26.0.0",
    "jest-preset-typescript": "1.2.0",
    "jest-preset-typescript-react-native": "1.2.0",
    "babel-jest": "26.0.1",
    "eslint": "7.2.0",
    "tslint": "6.1.2",
    "tslint-react": "5.0.0",
    "tslint-react-recommended": "1.0.15",
    "tslint-eslint-rules": "5.4.0",
    "html-webpack-plugin": "4.3.0",
    "jest": "26.0.1",
    "metro-react-native-babel-preset": "0.59.0",
    "prettier": "2.0.5",
    "react-native-typescript-transformer": "1.2.13",
    "react-test-renderer": "16.13.1",
    "typescript": "3.9.5",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": ["\\.snap$"],
    "cacheDirectory": ".jest/cache",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation|victory|rn-placeholder).+\\.js$"
    ],
    "moduleDirectories": ["<rootDir>", "node_modules"],
    "transform": {
      "^.+\\.(ts|tsx|js)$":
      "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "prettier": {
    "printWidth": 80
  }
}
